{
  "entryPoints": ["../extensions/src/**/*.ts"],
  "out": "../docs/extensions/",
  "plugin": ["typedoc-plugin-markdown", "typedoc-vitepress-theme"],
  "githubPages": false,
  "basePath": "./extensions/",
  "readme": "none",
  "indexFormat": "table",
  "useCodeBlocks": true,
  "disableSources": false,
  "excludeInternal": true,
  "excludeNotDocumented": true,
  "textContentMappings": {
    "title.indexPage": "API Reference",
    "title.memberPage": "{name}"
  },
  "docsRoot": "../docs/"
}
